<section >
    <h1 id="nintendo" class="titulares">Populares Nintendo</h1>
    <div class="caja-wrap">
        <% for(let i = 0; i < nintendoswitch.length; i ++) { %>
            <article class="caja-box-shadow">
                <a href="/products/<%=nintendoswitch[i].id%>">
                    <img  src="/images/index/<%=nintendoswitch[i].image%>" alt="imagen de producto">
                    <div class="caja">
                        <p><i class="fas fa-shipping-fast"></i></p>                           
                        <p class="precios"><%=nintendoswitch[i].price%></p>
                        <p class="descuentos"><%=nintendoswitch[i].discount%>% OFF</p>
                    </div>
                    <p class="descripciones"><%=nintendoswitch[i].name%></p>
                </a>
            </article>
        <% } %>                    
    </div>         
</section>